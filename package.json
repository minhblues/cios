{
    "name": "cios",
    "version": "1.0.0",
    "description": "A flexible and feature-rich HTTP client for TypeScript applications",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "unpkg": "dist/umd/cios.min.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "build": "rimraf dist && rollup -c",
        "build:dev": "rollup -c --environment BUILD:development",
        "build:prod": "rollup -c --environment BUILD:production",
        "dev": "rollup -c -w",
        "test": "jest",
        "lint": "eslint 'src/**/*.ts'",
        "format": "prettier --write 'src/**/*.ts'",
        "prepublishOnly": "npm run build",
        "clean": "rimraf dist"
    },
    "keywords": [
        "http",
        "client",
        "fetch",
        "api",
        "rest",
        "axios",
        "typescript",
        "request"
    ],
    "author": "Your Name",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/minhblues/cios.git"
    },
    "bugs": {
        "url": "https://github.com/minhblues/cios/issues"
    },
    "homepage": "https://github.com/minhblues/cios#readme",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^28.0.2",
        "@rollup/plugin-node-resolve": "^16.0.0",
        "@rollup/plugin-typescript": "^12.1.2",
        "@types/jest": "^29.5.0",
        "@types/node": "^18.15.5",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "eslint": "^8.36.0",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-fetch-mock": "^3.0.3",
        "prettier": "^2.8.6",
        "rimraf": "^4.4.1",
        "rollup": "^4.34.8",
        "rollup-plugin-dts": "^6.1.1",
        "ts-jest": "^29.0.5",
        "typescript": "^5.0.2"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "dependencies": {
        "@rollup/plugin-terser": "^0.4.4"
    }
}
